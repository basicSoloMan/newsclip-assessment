e={{ marginBottom: 20 }}>\n            <Button variant=\"dark\" onClick={() => setToggle(!toggle)}>\n              Add Model\n            </Button>\n          </div>\n\n          <ItemHeader>\n            <div>Model Code</div>\n            <div>Model Description</div>\n          </ItemHeader>\n\n          {isLoading && <div>Loading...</div>}\n\n          {!isLoading &&\n            models.map((model) => <Item key={model.item_id} model={model} />)}\n        </Card.Body>\n      </Card>\n      <Modal show={toggle} onHide={() => setToggle(!toggle)}>\n        <Modal.Header>Add a car model</Modal.Header>\n        <Modal.Body>\n          <Form>\n            <Form.Group>\n              <Form.Label>Model Code</Form.Label>\n              <Form.Control\n                name=\"itemCode\"\n                value={itemCode}\n                placeholder=\"Item code\"\n                onChange={(e) => handleChange(e)}\n                required\n              />\n            </Form.Group>\n            <Form.Group>\n              <Form.Label>Model Name</Form.Label>\n              <Form.Control\n                name=\"itemName\"\n                value={itemName}\n                onChange={(e) => handleChange(e)}\n                placeholder=\"Item name\"\n                required\n              />\n            </Form.Group>\n          </Form>\n        </Modal.Body>\n        <Modal.Footer>\n          <Button onClick={() => setToggle(!toggle)} variant=\"secondary\">\n            Cancel\n          </Button>\n          <Button onClick={(e) => handleSave(e)} variant=\"success\">\n            Save\n          </Button>\n        </Modal.Footer>\n      </Modal>\n    </div>\n  );\n};\n\nexport default Models;\n","/home/marius/Documents/newsclip-assessment/nca-front/src/App.js",["42"],"/home/marius/Documents/newsclip-assessment/nca-front/src/pages/Models/Styles.js",["43"],"/home/marius/Documents/newsclip-assessment/nca-front/src/pages/Variants/Variants.js",["44"],"/home/marius/Documents/newsclip-